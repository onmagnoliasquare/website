{
  "name": "frontend",
  "type": "module",
  "version": "0.7.3",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:development": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "wrangler dev",
    "deploy:dev": "wrangler deploy --env dev",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "test": "yarn test:unit && yarn test:integration",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:no-sync": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest run unit",
    "test-ci:integration": "playwright test integration",
    "test-ci:e2e": "playwright test e2e/",
    "test-ci:functional": "playwright test functional",
    "test-ci:unit": "vitest run unit",
    "test-ci:components": "vitest run components",
    "test:ci": "vitest run",
    "test:ci-no-pool": "vitest run --pool forks --poolOptions.forks.singleFork",
    "test:ui": "playwright test --ui .",
    "test-ci:production": "playwright test --project=production",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@portabletext/svelte": "^3.0.1",
    "@sanity/client": "^7.13.2",
    "@sanity/image-url": "^2.0.2",
    "@unpic/placeholder": "^0.1.2",
    "@unpic/svelte": "^1.0.1",
    "groq": "^4.21.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@sanity/types": "4.21.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.13.1",
    "playwright-core": "^1.57.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.0",
    "super-sitemap": "1.0.5",
    "svelte": "^5.46.0",
    "svelte-check": "^4.3.5",
    "svelte-eslint-parser": "1.4.1",
    "svelte-meta-tags": "^4.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "wrangler": "^4.54.0"
  }
}
