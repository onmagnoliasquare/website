{
  "name": "cms",
  "main": "package.json",
  "version": "0.6.5",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "preview": "wrangler dev dist",
    "build": "sanity build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "run -T vitest",
    "test-ci:unit": "run -T vitest run --pool forks --poolOptions.forks.singleFork",
