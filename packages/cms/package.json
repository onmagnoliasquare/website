    "react-is": "^19.2.0",
    "sanity": "^4.10.3",
    "sanity-plugin-media": "^4.0.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/cli": "^4.10.3",
    "@sanity/eslint-config-studio": "5.0.2",
    "@sanity/prettier-config": "2.0.1",
    "@types/react": "^19.2.2",
